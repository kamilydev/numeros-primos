#NUMEROS PRIMOS

num = int(input('digite um número: '))
c = 0
for i in range (1, num + 1):
    if num % i == 0:
        print('\033[34m', end='')
        c += 1
    else:
        print('\033[31m', end='')
    print('{} '.format(i), end='')
print('\n\033[o numero {} foi divisível {} vezes'.format(num, c))
if c == 2:
    print('O numero é primo')
else:
    print('o numero não é primo')
